{
  "name": "apper",
  "description": "Convention-based restful app development in node.js",
  "homepage": "https://github.com/anupbishnoi/apper",
  "version": "2.3.2",
  "author": {
    "name": "Anup Bishnoi",
    "email": "pixelsallover@gmail.com"
  },
  "dependencies": {
    "debug": "0.7.4",
    "underscore": "1.6.0",
    "portfinder": "0.2.1",
    "socket.io": "0.9.16",
    "express": "3.5.0",
    "requirejs": "2.1.11",
    "amdefine": "0.1.0",
    "uglify-js": "2.4.13",
    "shouldbe": "1.0.0",
    "firebase": "1.0.11",
    "json5": "0.2.0",
    "minimist": "0.2.0",
    "opener": "1.3.0"
  },
  "keywords": [
    "convention",
    "framework",
    "web",
    "rest",
    "restful",
    "router",
    "app",
    "api",
    "subapp",
    "mount"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/anupbishnoi/apper"
  },
  "main": "index",
  "bin": {
    "apper": "./bin/apper"
  },
  "scripts": {
    "prepublish": "npm prune",
    "test": "mocha test --recursive",
    "doc": "groc",
    "docpush": "groc --github",
